<AnchorPane fx:id="anchorPaneContent" prefHeight="800.0" prefWidth="1300.0" stylesheets="@first-scene-styles.css" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.vladislava_g.diplomatestingvm.controllers.ContentTest">
   <children>
      <ScrollPane fx:id="scrollPaneContent" fitToWidth="true" layoutX="176.0" layoutY="129.0" prefHeight="547.0" prefWidth="947.0">
         <content>
            <VBox fx:id="vboxContent" prefHeight="546.0" prefWidth="945.0" spacing="10.0" style="-fx-background-radius: 13px;" />
         </content>
      </ScrollPane>
      <Button fx:id="buttonBackContent" layoutX="62.0" layoutY="770.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="63.0" style="-fx-background-color: rgba(217, 217, 217, 0.65); -fx-background-radius: 20px;" text="Назад" />
      <Button fx:id="endTestButton" layoutX="176.0" layoutY="692.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="189.0" style="-fx-background-color: rgba(217, 217, 217, 1); -fx-background-radius: 20px;" text="Сдать тест" />
      <Button fx:id="buttonReset" layoutX="383.0" layoutY="692.0" mnemonicParsing="false" prefHeight="40.0" prefWidth="189.0" style="-fx-background-color: rgba(217, 217, 217, 1); -fx-background-radius: 20px;" text="Сбросить" />
      <HBox alignment="CENTER_RIGHT" layoutX="617.0" layoutY="704.0" prefHeight="19.0" prefWidth="508.0">
         <children>
            <Label fx:id="resultLabel" text="Вы ответили верно на 6 / 10 вопросов" visible="false">
               <font>
                  <Font size="13.0" />
               </font>
            </Label>
         </children>
      </HBox>
   </children>
</AnchorPane>
